<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Print Products Pricelist{% endblock %}</title>
        {% block stylesheets %}
            <link href="/thirdParty/bootstrap-4.5.0-dist/css/bootstrap.min.css" rel="stylesheet">
        {% endblock %}
    </head>
    <body>
    {% for flash_message in app.session.flashBag.get('error') %}
        <div class="flash-error">
            {{ flash_message|raw }}
        </div>
    {% endfor %}
    {% for flash_message in app.session.flashBag.get('warning') %}
        <div class="flash-warning">
            {{ flash_message|raw }}
        </div>
    {% endfor %}
    {% for flash_message in app.session.flashBag.get('notice') %}
        <div class="flash-notice">
            {{ flash_message|raw }}
        </div>
    {% endfor %}

    {% block navbar %}
        <nav class="container-fluid">
            <br>
            <a href="{{ path('home') }}">Home</a>
            <br>
            <br>
        </nav>
    {% endblock %}

        {% block body %}{% endblock %}
        {% block javascripts %}
            <script src="/thirdParty/jquery-3.5.1.min.js"></script>
        {% endblock %}
    </body>
</html>
